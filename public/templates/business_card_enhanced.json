{
  "version": "1.0",
  "name": "Business Card Enhanced",
  "description": "Professional business card with AI-generated background",
  "category": "business",
  "canvas": {
    "width": 350,
    "height": 200,
    "backgroundColor": "#ffffff",
    "format": "png",
    "dpi": 300
  },
  "inputs": {
    "name": {
      "type": "text",
      "required": true,
      "default": "John Doe",
      "description": "Full name"
    },
    "title": {
      "type": "text",
      "required": true,
      "default": "Software Engineer",
      "description": "Job title"
    },
    "company": {
      "type": "text",
      "required": true,
      "default": "Tech Corp",
      "description": "Company name"
    },
    "backgroundStyle": {
      "type": "text",
      "required": false,
      "default": "modern tech gradient",
      "description": "Background style description"
    },
    "ai_prompt": {
      "type": "ai-prompt",
      "required": false,
      "default": "Professional business background with subtle geometric patterns",
      "description": "AI Background Description",
      "placeholder": "Describe the background style you want (e.g., modern office, abstract patterns, nature themed)"
    },
    "ai_style": {
      "type": "ai-style",
      "required": false,
      "default": "corporate",
      "description": "Background Style"
    },
    "ai_negative": {
      "type": "ai-negative",
      "required": false,
      "default": "text, logos, people, cluttered",
      "description": "Avoid in Background"
    }
  },
  "layers": [
    {
      "id": "ai-background",
      "type": "ai-image",
      "visible": true,
      "zIndex": 0,
      "opacity": 0.8,
      "transform": {
        "x": 0,
        "y": 0,
        "width": 350,
        "height": 200
      },
      "content": {
        "prompt": "Professional business card background with ${backgroundStyle} style, subtle, elegant, clean corporate design, minimalist",
        "negativePrompt": "text, letters, words, logos, cluttered, busy, noisy, distracting",
        "fitMode": "cover",
        "fallbackSource": "data:image/svg+xml;base64,",
        "aiOperation": {
          "provider": "replicate",
          "operation": "generate", 
          "model": "nano-banana",
          "parameters": {
            "steps": 30,
            "guidance_scale": 7.5,
            "seed": 12345
          }
        }
      }
    },
    {
      "id": "overlay",
      "type": "shape",
      "visible": true,
      "zIndex": 1,
      "opacity": 0.1,
      "transform": {
        "x": 0,
        "y": 0,
        "width": 350,
        "height": 200
      },
      "content": {
        "fill": "#ffffff"
      }
    },
    {
      "id": "name-text",
      "type": "text",
      "visible": true,
      "zIndex": 2,
      "transform": {
        "x": 20,
        "y": 40,
        "width": 310,
        "height": 40
      },
      "content": {
        "text": "${name}",
        "fontFamily": "Arial, sans-serif",
        "fontSize": 24,
        "color": "#1a1a1a",
        "fontWeight": "bold"
      }
    },
    {
      "id": "title-text",
      "type": "text",
      "visible": true,
      "zIndex": 2,
      "transform": {
        "x": 20,
        "y": 80,
        "width": 310,
        "height": 25
      },
      "content": {
        "text": "${title}",
        "fontFamily": "Arial, sans-serif",
        "fontSize": 16,
        "color": "#666666"
      }
    },
    {
      "id": "company-text",
      "type": "text",
      "visible": true,
      "zIndex": 2,
      "transform": {
        "x": 20,
        "y": 105,
        "width": 310,
        "height": 25
      },
      "content": {
        "text": "${company}",
        "fontFamily": "Arial, sans-serif",
        "fontSize": 14,
        "color": "#888888"
      }
    }
  ],
  "outputs": [
    {
      "id": "main",
      "format": "png",
      "quality": 95,
      "dpi": 300
    }
  ],
  "metadata": {
    "tags": ["business", "card", "professional", "ai-background"],
    "category": "business",
    "author": "Template System",
    "createdAt": 1703980800000
  }
}